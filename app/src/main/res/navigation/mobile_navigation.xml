<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_google">

    <fragment
        android:id="@+id/navigation_google"
        android:name="kr.co.js.trend_news.ui.google.GoogleFragment"
        android:label="@string/tab_title_google"
        tools:layout="@layout/fragment_google" />

    <fragment
        android:id="@+id/navigation_naver"
        android:name="kr.co.js.trend_news.ui.naver.NaverFragment"
        android:label="@string/tab_title_naver"
